{
  "name": "mymapp",
  "version": "0.3.1",
  "author": "Andr35",
  "description": "A simple application to record travels, trips, and other activities with attached photos into a geojson file.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "precap:sync:prod": "npm run build:prod",
    "cap:sync:prod": "cap sync",
    "test": "ng test",
    "lint": "ng lint && stylelint './**/*.scss'",
    "e2e": "ng e2e",
    "release:patch": "standard-version --release-as patch --bumpFiles package.json electron/package.json",
    "release:minor": "standard-version --release-as minor --bumpFiles package.json electron/package.json",
    "check-deps": "npm-check -s -i typescript -i @types/node"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^11.0.1",
    "@angular/common": "^11.0.3",
    "@angular/core": "^11.0.3",
    "@angular/forms": "^11.0.3",
    "@angular/platform-browser": "^11.0.3",
    "@angular/platform-browser-dynamic": "^11.0.3",
    "@angular/router": "^11.0.3",
    "@angular/service-worker": "^11.0.3",
    "@capacitor-community/electron": "^1.3.2",
    "@capacitor/core": "^2.4.4",
    "@ionic/angular": "^5.5.1",
    "@mapbox/mapbox-sdk": "^0.11.0",
    "@mdi/svg": "^5.8.55",
    "@ngxs-labs/actions-executing": "^0.1.3",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "date-fns": "^2.16.1",
    "exif-js": "^2.3.0",
    "file-saver": "^2.0.5",
    "mapbox-gl": "^1.13.0",
    "mapbox-gl-controls": "^1.6.5",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "uuid": "^8.3.1",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.3",
    "@angular/cli": "^11.0.3",
    "@angular/compiler": "^11.0.3",
    "@angular/compiler-cli": "^11.0.3",
    "@angular/language-service": "^11.0.3",
    "@capacitor/cli": "^2.4.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ionic/angular-toolkit": "^3.0.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/mapbox-gl": "^1.12.8",
    "@types/mapbox__mapbox-sdk": "^0.11.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "codelyzer": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "devmoji": "^2.1.12",
    "husky": "^4.3.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.5.3",
    "npm-check": "^5.9.2",
    "protractor": "^7.0.0",
    "standard-version": "^9.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "ts-node": "^9.1.0",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "devmoji -e"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.scss": "stylelint",
    "*.ts": "tslint",
    "*.spec.ts": "tslint"
  }
}
